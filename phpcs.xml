<?xml version="1.0" encoding="UTF-8"?>
<ruleset name="Yii2">
    <config name="encoding" value="utf-8"/>
    <config name="error_severity" value="1"/>
    <config name="warning_severity" value="1"/>
    <config name="ignore_warnings_on_exit" value="1"/>
    <rule ref="PSR2">
        <exclude name="Generic"/>
        <exclude name="Squiz"/>
        <exclude name="PSR1"/>
    </rule>
</ruleset>
